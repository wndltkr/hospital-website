{
  "hosting": {
    "public": "out",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
    { "source": "/admin", "destination": "/admin.html", "type": 301 },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
} 